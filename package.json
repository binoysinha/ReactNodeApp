  "bugs": {
    "url": "https://github.com/binoysinha/ReactNodeApp/issues"
  },
  "homepage": "https://github.com/binoysinha/ReactNodeApp#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.2",
    "mongoose": "^5.0.0-rc0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "sendgrid": "^5.2.3",
    "stripe": "^5.4.0"
  }
}
